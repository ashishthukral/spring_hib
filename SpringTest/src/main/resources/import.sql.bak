drop table stock if exists;

create table stock (STOCK_ID integer generated by default as identity (start with 1), STOCK_CODE varchar(10) not null, STOCK_NAME varchar(20) not null,primary key (STOCK_ID), unique (STOCK_CODE),unique (STOCK_NAME));

